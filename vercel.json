{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "csv_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/csv_server.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/$1"
    }
  ],
  "functions": {
    "csv_server.py": {
      "runtime": "python3.11"
    }
  },
  "installCommand": "cd frontend && npm install && npm run build",
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist"
}
